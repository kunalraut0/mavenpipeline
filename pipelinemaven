pipeline {
    agent any 
    stages {
        stage('pull') { 
            steps {
                git branch: 'main', url: 'https://github.com/kunalraut0/kunal-maven-build.git'
            }
        }
        
        stage('build') { 
            steps {
               sh 'mvn clean package'
            }
        } 
        
        stage('test') { 
            steps {
                echo 'test successful'
            }
        }
        
        stage('deploy') { 
            steps {
               echo 'deploy successful'
            }
        } 
    }
}    
            
